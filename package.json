{
  "icon": "icon.png",
  "name": "bit-theme",
  "displayName": "Bit Theme",
  "description": "A Dark Theme created by pedro-henrique-sb.",
  "homepage": "https://github.com/bit-theme/bit-theme",
  "version": "1.0.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bit-theme/visual-studio-code.git"
  },
  "bugs": {
    "url": "https://github.com/bit-theme/visual-studio-code/issues"
  },
  "publisher": "bit-theme",
  "author": {
    "name": "Pedro Henrique",
    "email": "pyter.dev@gmail.com",
    "url": "https://github.com/pedro-henrique-sb"
  },
  "maintainers": [
    "Pedro Henrique <pyter.dev@gmail.com>"
  ],
  "engines": {
    "node": ">=12",
    "vscode": "^1.65.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "Bit",
    "Bit Theme",
    "Bit Themes",
    "bit",
    "bit theme",
    "theme",
    "color-theme",
    "color theme",
    "dark"
  ],
  "galleryBanner": {
    "color": "#131313",
    "theme": "dark"
  },
  "contributes": {
    "themes": [
      {
        "label": "Bit",
        "uiTheme": "vs-dark",
        "path": "./themes/bit-default.json"
      },
      {
        "label": "Bit Intense",
        "uiTheme": "vs-dark",
        "path": "./themes/bit-intense.json"
      },
      {
        "label": "Bit Soft",
        "uiTheme": "vs-dark",
        "path": "./themes/bit-soft.json"
      }
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint .",
    "dev": "ts-node-dev --respawn --rs dist/scripts/build.js",
    "build:ts": "yarn lint && yarn tsc",
    "build": "yarn build:ts && node dist/scripts/build.js",
    "package": "vsce package",
    "vscode:prepublish": "yarn build",
    "vsce:publish": "vsce publish"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.10.0",
    "husky": "^7.0.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2",
    "vsce": "^2.7.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "__metadata": {}
}
